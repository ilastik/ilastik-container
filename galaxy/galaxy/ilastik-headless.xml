<tool id="ilastik-headless" name="ilastik" version="0.1.0" python_template_version="3.5">
    <requirements>
        <container type="docker">ilastik/ilastik-galaxy:1.3.3post3</container>
    </requirements>
    <command>
        /usr/local/bin/run_ilastik.py pixel_classification --project '$input1' --raw_data '$input2' --output_filename_format '$output1'
    </command>
    <inputs>
        <param type="data" name="input1" format="ilp" />
        <param type="data" name="input2" format="png" />
    </inputs>
    <outputs>
        <data name="output1" format="h5" />
    </outputs>
    <tests>
        <test>
            <param name="input1" value="2dcellsapo_1.3.2-Linux.ilp"/>
            <param name="input2" value="2d_cells_apoptotic_1channel.png"/>
            <output name="output1" file="2d_cells_apoptotic_1channel-Probabilities.h5"/>
        </test>
    </tests>
    <help><![CDATA[
        TODO: Fill in help.
    ]]></help>
</tool>